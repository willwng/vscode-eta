{
  "name": "vscode-eta",
  "displayName": "vscode-eta",
  "description": "VSCode Eta language support",
  "version": "0.0.3",
  "publisher": "wackymaster",
  "icon": "icon.png",
  "repository": {
    "url": "https://github.com/willwng/vscode-eta"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Programming Languages", "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "eta",
        "aliases": [
          "Eta",
          "eta"
        ],
        "extensions": [
          ".eta",
          ".eti"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eta",
        "scopeName": "text.eta",
        "path": "./syntaxes/eta.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "eta",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  }
}